@model ServerMDA.ViewModel.CMovieLanguageViewModel

@{
    ViewData["Title"] = "修改";
}

<h4 class="offset-md-1">修改電影語言編號 : @Model.語言編號languageId</h4>

<div class="row">
    <div class="col-md-5 offset-md-1">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="語言編號languageId" />
            <div class="form-group">
                <label asp-for="語言名稱languageName" class="control-label"></label>
                <input asp-for="語言名稱languageName" class="form-control" />
                <span asp-validation-for="語言名稱languageName" class="text-danger"></span>
            </div>
            <div class="form-group" style="float:right">
                <input type="submit" value="修改" class="btn btn-yuka mr-2" />
                <a asp-action="List" class="btn btn-secondary">回到清單</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
